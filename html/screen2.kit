    <script>

        function toggleExpandCollapse(button) {
            const row = button.parentNode.parentNode;
            const nextRow = row.nextElementSibling;
            if (nextRow && nextRow.classList.contains('details-row')) {
                if (nextRow.style.display === 'none') {
                    nextRow.style.display = 'flex';
                    button.innerHTML = ' <img src="./dist/img/panel-arrow-reverse.svg" alt="">';
                } else {
                    nextRow.style.display = 'none';
                    button.innerHTML = ' <img src="./dist/img/panel-arrow.svg" alt="">';
                }
            }
        }

        const checkbox = document.querySelectorAll(".checkbox");
        const allCheckbox = document.getElementById("changeAllCheckbox");
        const divs = document.querySelectorAll(".panelTable");

        checkbox.forEach(box => {
            box.addEventListener("change", function () {
                const container = box.closest('.checkbox-container');
                const index = container.getAttribute('data-index');
                const div = document.querySelector(`.panelTable[data-index='${index}']`);

                if (box.checked) {
                    div.style.backgroundColor = "#EFF6FF";
                    div.style.borderColor = "#3B82F6";
                } else {
                    div.style.backgroundColor = "#fff";
                    div.style.borderColor = "#d1d5db";
                }
            });
        });

        allCheckbox.addEventListener("change", function () {
            if (allCheckbox.checked) {
                divs.forEach(div => {
                    div.style.backgroundColor = "#EFF6FF";
                    div.style.borderColor = "#3B82F6";
                })
                checkbox.forEach(box => {
                    box.checked = true
                })
            } else {
                divs.forEach(div => {
                    div.style.backgroundColor = "#fff";
                    div.style.borderColor = "#d1d5db";
                })
                checkbox.forEach(box => {
                    box.checked = false
                })
            }
        });


        document.addEventListener('DOMContentLoaded', function () {
            var dropdownSubmenus = document.querySelectorAll('.dropdown-submenu .dropdown-toggle');
            dropdownSubmenus.forEach(function (submenuToggle) {
                submenuToggle.addEventListener('click', function (e) {
                    e.stopPropagation();
                    e.preventDefault();

                    var submenu = this.nextElementSibling;
                    if (submenu.classList.contains('show')) {
                        submenu.classList.remove('show');
                    } else {
                        var openSubmenus = document.querySelectorAll('.dropdown-submenu .dropdown-menu.show');
                        openSubmenus.forEach(function (openSubmenu) {
                            openSubmenu.classList.remove('show');
                        });
                        submenu.classList.add('show');
                    }
                });
            });

            function toggleRow(row) {
                row.classList.toggle('selected');
            }

            document.addEventListener('click', function () {
                var openSubmenus = document.querySelectorAll('.dropdown-submenu .dropdown-menu.show');
                openSubmenus.forEach(function (openSubmenu) {
                    openSubmenu.classList.remove('show');
                });
            });
        });
    </script>
